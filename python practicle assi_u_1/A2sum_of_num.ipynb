    "#Assignment 2-find the sum of first n natural number\n",
    "def sum(n):\n",
    "    total=0\n",
    "    for i in range(1,n+1):\n",
    "        total +=i\n",
    "    return total\n",
    "n=10\n",
    "print(f\"the sum of first {n} natural numbers is:{sum(n)}\")\n",
